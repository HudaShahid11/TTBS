<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kewo.youthservices">
    <supports-screens
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="false"
        android:xlargeScreens="false"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="com.kewo.youthservices.cat_welder"></activity>
        <activity android:name="com.kewo.youthservices.cat_refrigeration" />
        <activity android:name="com.kewo.youthservices.cat_motor_pump" />
        <activity android:name="com.kewo.youthservices.cat_generator" />
        <activity android:name="com.kewo.youthservices.cat_fitter" />
        <activity android:name="com.kewo.youthservices.cat_electronics" />
        <activity android:name="com.kewo.youthservices.cat_electrician" />
        <activity android:name="com.kewo.youthservices.cat_computer" />
        <activity android:name="com.kewo.youthservices.cat_auto" />
        <activity android:name="com.kewo.youthservices.service_provider_form" />
        <activity android:name="com.kewo.youthservices.feedback" />
        <activity android:name="com.kewo.youthservices.spashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kewo.youthservices.ServiceActivity" />
        <activity android:name="com.kewo.youthservices.CourseActivity" />
        <activity android:name="com.kewo.youthservices.CourseDetail" />
        <activity android:name="com.kewo.youthservices.ServiceDetail" />
        <activity android:name="com.kewo.youthservices.list" />
        <activity android:name="com.kewo.youthservices.home" />
        <activity android:name="com.kewo.youthservices.signup" />
        <activity android:name="com.kewo.youthservices.login" />
        <activity android:name="com.kewo.youthservices.register" />
        <activity android:name="com.kewo.youthservices.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>